<div className=\"inner-wrapper\">\n          <div className=\"vocab\">{vocab}</div>\n          <div className=\"meaning\">{meaning}</div>\n          <div className=\"note\" dangerouslySetInnerHTML={{ __html: note }} />\n        </div>\n      </div>\n    )\n  }\n\n  render() {\n    const { vocabArr } = this.state;\n    return (\n      <div className=\"vocab-reviewer\">\n        <div className=\"total-count\">{data.length}</div>\n        <div className=\"vocab-list\">\n          {\n            vocabArr.map((item, index) => {\n              return (\n                <div className=\"vocab-card\" key={index} onClick={() => this.onVocabClick(item)}>{item.vocab}</div>\n              )\n            })\n          }\n        </div>\n        <div className=\"refresh-btn\" onClick={this.getRandomVocabShuffle}>REFRESH</div>\n        {this.renderModal()}\n      </div>\n    );\n  }\n}\n\nexport default VocabReviewer;\n",["73","74"],"/Users/napolizhou/Desktop/My repos/my-tools/src/pages/AudioList/data.js",[],"/Users/napolizhou/Desktop/My repos/my-tools/src/pages/VocabReviewer/data/notes.js",[],"/Users/napolizhou/Desktop/My repos/my-tools/src/pages/Linklist/index.js",["75","76"],"import React, { Component } from 'react';\n\nimport hnList from './huanong';\nimport jyList from './jinyang';\n\nimport './index.css';\n\nclass Linklist extends Component {\n\trenderList(list = []) {\n\t\treturn (\n\t\t\t<div>\n\t\t\t\t{\n\t\t\t\t\tlist.map((item,index) => {\n\t\t\t\t\t\treturn (\n\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t<a href={item.href} target=\"_blank\" key={index}>{item.label}</a>\n\t\t\t\t\t\t\t\t<br/>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t)\n\t\t\t\t\t})\n\t\t\t\t}\n\t\t\t</div>\n\t\t)\n\t}\n\n  render() {\n    return (\n      <div className=\"link-list-wrapper\"> \n\t\t\t\t{this.renderList(hnList)}\n\t\t\t</div>\n    );\n  }\n}\n\nexport default Linklist;\n","/Users/napolizhou/Desktop/My repos/my-tools/src/pages/VocabReviewer/data/tem8-3.js",[],"/Users/napolizhou/Desktop/